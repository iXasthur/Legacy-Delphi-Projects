program Lab2;

{$APPTYPE CONSOLE}

{$R *.res}

uses
System.SysUtils;




procedure calcY(x, pogr:real);
var
  n1, n2, k:Integer;
  y, saveSum: real;
begin
  n1:=10;
  n2:=15;

  for k := n1 to n2 do
  begin
    y:=x;
    writeln('     y=',y:0:10,' (k=',k,')');
  end;
end;






var
  pogr, x1, x2, dx, x, y:Real;

begin
  x1:=0.6;
  x2:=1.1;
  dx:=0.25;

  pogr:=dx/1000;

  x:=x1;
  while x<=x2+pogr do
  begin
    writeln('x=',x:0:2);
    calcY(x, pogr);
    x:= x+dx;
  end;




  writeln('Press <Enter> to exit');
  readln;
end.
